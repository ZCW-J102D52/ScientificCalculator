

public static double logValue(double x,double base){

    double result = Math.log(x) / Math.log(base);
    return result;

    }


public static double inverseLog(double x){
    double result = Math.exp(Math.log(x);
    return result
}

public static double naturalLog(double x){
    double result = Math.log(x);
    return result
}

public static double inverseNatLog(double x){
    double result = Math.exp(x)
    return result
}



